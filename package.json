{
  "name": "task21",
  "version": "1.0.0",
  "description": "boilerplate for react and express",
  "main": "server.js",
  "scripts": {
    "frontend-install": "cd frontend && npm install",
    "test": "mocha",
    "start": "node server.js",
    "server": "nodemon server.js",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "cd frontend && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "author": "Safwaan Abrams",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "express-router": "0.0.1",
    "helmet": "^4.1.0",
    "i": "^0.3.6",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "react-helmet": "^6.1.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "request": "^2.88.2",
    "requests": "^0.3.0",
    "router": "^1.3.5"
  },
  "engines": {
    "node": "12.18.2",
    "npm": "6.14.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "gh-pages": "^3.1.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4"
  }
}
